		<param-name>driverClass</param-name>
		<param-value>com.mysql.jdbc.Driver</param-value>
	</context-param>
	<context-param>
		<param-name>connectionURL</param-name>
		<param-value>jdbc:mysql://localhost/uimptutor</param-value>
	</context-param>
	<context-param>
		<param-name>username</param-name>
		<param-value>root</param-value>
	</context-param>
	<context-param>
		<param-name>password</param-name>
		<param-value>!j#root@</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyTestKey</param-name>
		<param-value>fB7m8s</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyTestSalt</param-name>
		<param-value>eRis5Chv</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyTestUrl</param-name>
		<param-value>https://test.payu.in</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyLiveKey</param-name>
		<param-value>fB7m8s</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyLiveSalt</param-name>
		<param-value>eRis5Chv</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyLiveUrl</param-name>
		<param-value>https://test.payu.in</param-value>
	</context-param>
	<context-param>
		<param-name>payuMoneyMode</param-name>
		<param-value>test</param-value>
	</context-param>
</web-app>
